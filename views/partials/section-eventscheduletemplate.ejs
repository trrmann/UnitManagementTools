<section id="eventscheduletemplate" class="section">
  <h2>Event Schedule Templates</h2>
  <div class="section-toolbar eventscheduletemplate-toolbar improved-toolbar">
    <div class="eventscheduletemplate-toolbar-row">
      <input type="text" id="eventscheduletemplateSearch" class="eventscheduletemplate-search" placeholder="Search event schedule templates..." />
      <div class="eventscheduletemplate-toolbar-buttons">
        <button class="btn-secondary" id="eventscheduletemplateImportBtn"><i class="fas fa-file-import"></i> Import</button>
        <button class="btn-secondary" id="eventscheduletemplateExportBtn"><i class="fas fa-file-export"></i> Export</button>
        <button class="btn-primary eventschedule-AddTemplate" id="eventscheduletemplateAddBtn" onclick="openAddEventScheduleTemplate()"><i class="fas fa-plus"></i> Add</button>
      </div>
    </div>
  </div>
  <div id="eventscheduletemplateTable" class="eventscheduletemplate-table">
    <table>
      <thead>
        <tr>
          <th>Template Name</th>
          <th>Description</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody id="eventscheduletemplateBody">
        <!-- Event schedule template rows will be dynamically generated by JavaScript -->
      </tbody>
    </table>
  </div>
</section>
